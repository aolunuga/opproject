<service name="ResourceService" class="onepoint.project.modules.resource.OpResourceService">
   <method name="insertResource">
      <parameter name="resource_data" instance-class="java.util.HashMap" mode="input"/>
   </method>
   <method name="importUser">
      <parameter name="resource_data" instance-class="java.util.HashMap" mode="input"/>
   </method>
   <method name="updateResource">
      <parameter name="resource_id" instance-class="java.lang.String" mode="input"/>
      <parameter name="resource_data" instance-class="java.util.HashMap" mode="input"/>
   </method>
   <method name="deleteResources">
      <parameter name="resource_ids" instance-class="java.util.ArrayList" mode="input"/>
   </method>
   <method name="hasAssignments">
      <parameter name="resource_id" instance-class="java.lang.String" mode="input"/>
   </method>
   <method name="hasResourceAssignments">
      <parameter name="pool_id" instance-class="java.lang.String" mode="input"/>
   </method>
   <method name="insertPool">
      <parameter name="pool_data" instance-class="java.util.HashMap" mode="input"/>
   </method>
   <method name="updatePool">
      <parameter name="pool_id" instance-class="java.lang.String" mode="input"/>
      <parameter name="pool_data" instance-class="java.util.HashMap" mode="input"/>
   </method>
   <method name="deletePools">
      <parameter name="pool_ids" instance-class="java.util.ArrayList" mode="input"/>
   </method>
   <method name="assignToProject">
      <parameter name="resource_ids" instance-class="java.util.ArrayList" mode="input"/>
      <parameter name="project_ids" instance-class="java.util.ArrayList" mode="input"/>
   </method>
   <method name="moveResourceNode">
      <parameter name="resource_ids" instance-class="java.util.ArrayList" mode="input"/>
      <parameter name="pool_id" instance-class="java.lang.String" mode="input"/>
   </method>
   <method name="movePoolNode">
      <parameter name="pool_ids" instance-class="java.util.ArrayList" mode="input"/>
      <parameter name="super_pool_id" instance-class="java.lang.String" mode="input"/>
   </method>
</service>
