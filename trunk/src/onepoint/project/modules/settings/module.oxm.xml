<module name="settings" caption="Einstellungen" version="0.1" class="onepoint.project.modules.settings.OpSettingsModule">
   <prototype-files>
      <prototype-file file-name="/modules/settings/setting.opt.xml"/>
   </prototype-files>
   <service-files>
      <service-file file-name="/modules/settings/settings_service.osd.xml"/>
   </service-files>
   <language-kit-paths>
      <language-kit-path path="/modules/settings/i18n"/>
   </language-kit-paths>
   <tool-groups>
      <tool-group name="system" caption="${SystemToolGroup}" category="system" administrator-only="true" sequence="1"/>
   </tool-groups>
   <tools>
      <tool name="settings" caption="${SettingsTool}" group-ref="system" icon="/modules/settings/icons/settings_tool.png" start-form="/modules/settings/forms/settings.oxf.xml" sequence="0"/>
   </tools>
</module>
