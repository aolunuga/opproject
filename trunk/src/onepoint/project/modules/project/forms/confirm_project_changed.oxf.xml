<form layout="border" default-button-ref="OKButton" resource-map-ref="project.change" text="${ConfirmChange}" provider-class="onepoint.project.modules.project.forms.OpConfirmChangeProjectFormProvider" script="/modules/project/scripts/confirm_project_changed.jes">
   <panel y="0" layout="border">
      <data-field id="project_id"/>
      <data-field id="project_data"/>
      <data-field id="ModifiedRates"/>
      <data-set id="goals_set"/>
      <data-set id="resource_set"/>
      <data-set id="versions_set"/>
      <data-set id="attachment_set"/>

      <label y="0" id="ConfirmChangeLabel"/>
      <panel y="1" layout="flow" direction="west">
         <button id="CancelButton" text="${Cancel}" on-button-pressed="cancel"/>
         <button id="OKButton" text="${OK}" on-button-pressed="ok"/>
      </panel>
   </panel>
</form>