<service name="ReportService" class="onepoint.project.modules.report.OpReportService" interceptor="onepoint.project.OpServiceInterceptor">
   <method name="createReport">
      <parameter name="name" instance-class="java.lang.String" mode="input"/>
      <parameter name="parameters" instance-class="java.util.HashMap" mode="input"/>
      <parameter name="formats" instance-class="java.util.ArrayList" mode="input"/>
   </method>
   <method name="saveReport">
      <parameter name="name" instance-class="java.lang.String" mode="input"/>
      <parameter name="parameters" instance-class="java.util.HashMap" mode="input"/>
      <parameter name="formats" instance-class="java.util.ArrayList" mode="input"/>
   </method>
   <method name="sendReport">
      <parameter name="name" instance-class="java.lang.String" mode="input"/>
      <parameter name="parameters" instance-class="java.util.HashMap" mode="input"/>
      <parameter name="formats" instance-class="java.util.ArrayList" mode="input"/>
   </method>
   <method name="setSessionReportQueryType">
      <parameter name="reportQueryType" instance-class="java.lang.String" mode="input"/>
   </method>
   <method name="reportsCleanUp">
   </method>
</service>
