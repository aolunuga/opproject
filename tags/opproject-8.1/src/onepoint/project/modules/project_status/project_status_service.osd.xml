<service name="ProjectStatusService" class="onepoint.project.modules.project_status.OpProjectStatusService" interceptor="onepoint.project.modules.user.OpPermissionCheckServiceInterceptor">
   <method name="insertProjectStatus" adminCheck="true">
      <parameter name="project_status_data" instance-class="java.util.HashMap" mode="input"/>
   </method>
   <method name="updateProjectStatus" adminCheck="true">
      <parameter name="project_status_id" instance-class="java.lang.String" mode="input"/>
      <parameter name="project_status_data" instance-class="java.util.HashMap" mode="input"/>
   </method>
   <method name="deleteProjectStatus" adminCheck="true">
      <parameter name="project_status_ids" instance-class="java.util.ArrayList" mode="input"/>
   </method>
   <method name="move" adminCheck="true">
      <parameter name="project_locators" instance-class="java.util.ArrayList" mode="input"/>
   </method>
</service>
