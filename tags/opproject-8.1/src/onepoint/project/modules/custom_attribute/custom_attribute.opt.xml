<prototype name="OpCustomAttribute" instance-class="onepoint.project.modules.custom_attribute.OpCustomAttribute">
   <field name="Name" type="String" mandatory="true" indexed="true" />
   <field name="Description" type="String" />
   <field name="Label" type="String" />
   <field name="Type" type="Integer" mandatory="true" />
   <field name="Unique" type="Boolean" />
   <field name="Mandatory" type="Boolean" />
   <field name="Deleted" type="Boolean" indexed="true" />
   <field name="Position" type="Integer" indexed="true" />
   <field name="Sequence" type="Integer" ordered="true"/>
   <field name="MinRole" type="Byte" />
   <field name="PossibleStringValues" type="String" />
   <relationship name="CustomType" type="OpCustomType" back-relationship="CustomAttributes"/>
</prototype>
