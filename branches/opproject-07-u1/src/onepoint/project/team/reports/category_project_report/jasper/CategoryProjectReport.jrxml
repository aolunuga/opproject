	<queryString><![CDATA[select ac.*,
pn.op_name AS pn_name, pn.op_description AS pn_description,
cat.op_name AS cat_name, cat.op_description AS cat_description
from op_activity ac
inner join op_projectplan pp on ac.op_projectplan = pp.op_id
inner join op_projectnode pn on pp.op_projectnode = pn.op_id
inner join op_activitycategory cat on ac.op_category = cat.op_id
where ac.op_finish >= $P{fromDate} and ac.op_start <= $P{toDate} and ac.op_deleted = 0 and ac.op_template = 0 
and pp.op_projectnode = $P{ProjectId}
order by cat.op_name, ac.op_start, pn.op_name]]></queryString>

