	<parameter name="UserId" isForPrompting="true" class="java.lang.Long">
		<parameterDescription><![CDATA[op_id of User from op_subject]]></parameterDescription>
	</parameter>
	<parameter name="fromDate" isForPrompting="true" class="java.util.Date">
		<parameterDescription><![CDATA[Start-Date for selection from op_workslip]]></parameterDescription>
		<defaultValueExpression ><![CDATA[new Date("01/01/2006 00:00")]]></defaultValueExpression>
	</parameter>
	<parameter name="toDate" isForPrompting="true" class="java.util.Date">
		<parameterDescription><![CDATA[End-Date fror selction from op_workslip]]></parameterDescription>
		<defaultValueExpression ><![CDATA[new Date("03/31/2006 23:59")]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[select wr.*, ws.op_date AS ws_date, s.op_name AS s_name, s.op_displayname AS s_displayname,
rs.op_name AS rs_sub_name, rs.op_description AS rs_sub_description,
rs.op_name AS rs_name, rs.op_description AS rs_description, 
ac.op_name AS ac_name, ac.op_description AS ac_description,
pn.op_name AS pn_name, pn.op_description AS pn_description

from op_workslip ws

inner join op_workrecord wr on wr.op_workslip = ws.op_id
inner join op_assignment a on wr.op_assignment = a.op_id
inner join op_activity ac on a.op_activity = ac.op_id
inner join op_projectplan pp on ac.op_projectplan = pp.op_id
inner join op_projectnode pn on pp.op_projectnode = pn.op_id
inner join op_subject s on ws.op_creator = s.op_id
inner join op_resource rs on s.op_id = rs.op_user

where a.op_resource = rs.op_id and pn.op_archived=false and ws.op_date >= $P{fromDate} and ws.op_date <= $P{toDate}
and s.op_id = $P{UserId}
order by rs.op_name, pn.op_name, ws.op_date]]></queryString>

